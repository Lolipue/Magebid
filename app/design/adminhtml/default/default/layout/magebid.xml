<layout>
	<default>
		<reference name="head">
		    <action method="addJs"><script>magebid/magebid.js</script></action>
		</reference> 	
	</default>		
	
	<mbid_magebid_adminhtml_configuration_main_index>
		<reference name="head">
			<action method="addCss"><name>magebid.css</name></action>
		</reference>
	</mbid_magebid_adminhtml_configuration_main_index>	
	
	<mbid_magebid_adminhtml_configuration_main_save>
		<reference name="head">
			<action method="addCss"><name>magebid.css</name></action>
		</reference>
	</mbid_magebid_adminhtml_configuration_main_save>
	
    <mbid_magebid_adminhtml_export_main_massprepare>
        <reference name="left">
            <block type="adminhtml/store_switcher" name="store_switcher" before="-"></block>
        </reference>	
    </mbid_magebid_adminhtml_export_main_massprepare>	
	
    <mbid_magebid_adminhtml_catalog_main_index>
        <reference name="content">
            <block type="adminhtml/store_switcher" name="store_switcher" before="-"></block>
        </reference>	
    </mbid_magebid_adminhtml_catalog_main_index>		
</layout>





